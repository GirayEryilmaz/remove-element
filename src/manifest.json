{
  "name": "RemoveElement",
  "version": "0.0.5",
  "description": "Remove HTML elements from the right-click context menu",
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "19": "icons/icon-19.png",
    "16": "icons/icon-16.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/removeElement.js"
      ]
    }
  ],
  "manifest_version": 2
}